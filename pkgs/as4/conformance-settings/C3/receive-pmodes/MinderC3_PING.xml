<?xml version="1.0"?>
<PMode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
  xmlns="eu:edelivery:as4:pmode">
  <Id>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/test</Id>
  <Reliability>
    <DuplicateElimination>
      <IsEnabled>false</IsEnabled>
    </DuplicateElimination>
  </Reliability>
  <ReplyHandling>
    <ReplyPattern>Response</ReplyPattern>
    <ReceiptHandling>
      <UseNRRFormat>true</UseNRRFormat>
    </ReceiptHandling>
    <ErrorHandling>
      <UseSoapFault>false</UseSoapFault>
      <ResponseHttpCode>500</ResponseHttpCode>
    </ErrorHandling>
    <ResponseConfiguration>
      <Protocol>
        <Url>http://13.81.109.44:15001/corner4</Url>
        <UseChunking>false</UseChunking>
        <UseHttpCompression>false</UseHttpCompression>
      </Protocol>
    </ResponseConfiguration>
    <ResponseSigning>
      <IsEnabled>true</IsEnabled>
      <CertificateFindCriteria>
        <CertificateFindType>FindBySubjectName</CertificateFindType>
        <CertificateFindValue>as4-net-c3</CertificateFindValue>
      </CertificateFindCriteria>
      <KeyReferenceMethod>BSTReference</KeyReferenceMethod>
      <Algorithm>http://www.w3.org/2001/04/xmldsig-more#rsa-sha256</Algorithm>
      <HashFunction>http://www.w3.org/2001/04/xmlenc#sha256</HashFunction>
    </ResponseSigning>
  </ReplyHandling>
  <ExceptionHandling>
    <NotifyMessageConsumer>false</NotifyMessageConsumer>
    <NotifyMethod />
  </ExceptionHandling>
  <Security>
    <SigningVerification>
      <Signature>Allowed</Signature>
    </SigningVerification>
    <Decryption>
      <Encryption>Allowed</Encryption>
      <CertificateFindCriteria>
        <CertificateFindType>FindBySubjectName</CertificateFindType>
        <CertificateFindValue>as4-net-c3</CertificateFindValue>
      </CertificateFindCriteria>
    </Decryption>
  </Security>
  <MessagePackaging>
    <PartyInfo>
      <FromParty>
        <PartyIds>
          <PartyId>
            <Id>as4-net-c2</Id>
            <Type>urn:oasis:names:tc:ebcore:partyid-type:unregistered</Type>
          </PartyId>
        </PartyIds>
        <Role>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/initiator</Role>
      </FromParty>
      <ToParty>
        <PartyIds>
          <PartyId>
            <Id>as4-net-c3</Id>
            <Type>urn:oasis:names:tc:ebcore:partyid-type:unregistered</Type>
          </PartyId>
        </PartyIds>
        <Role>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/responder</Role>
      </ToParty>
    </PartyInfo>
    <CollaborationInfo>
      <Action>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/test</Action>
      <Service>
        <Value>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/service</Value>
      </Service>
    </CollaborationInfo>
  </MessagePackaging>
  <MessageHandling>
    <Deliver>
      <IsEnabled>true</IsEnabled>
      <PayloadReferenceMethod>
        <Type>FILE</Type>
        <Parameters>
          <Parameter name="location" value=".\messages\in\" />
          <Parameter name="AllowOverwrite" value="true" />
        </Parameters>
      </PayloadReferenceMethod>
      <DeliverMethod>
        <Type>HTTP</Type>
        <Parameters>
          <Parameter name="location" value="http://13.81.109.44:15001/corner4" />
        </Parameters>
      </DeliverMethod>
    </Deliver>
  </MessageHandling>
</PMode>