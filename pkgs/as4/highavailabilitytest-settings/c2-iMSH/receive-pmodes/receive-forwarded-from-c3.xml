<?xml version="1.0"?>
<PMode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="eu:edelivery:as4:pmode">
  <Id>receive-forwarded-from-c3</Id>
  <Reliability>
    <DuplicateElimination>
      <IsEnabled>false</IsEnabled>
    </DuplicateElimination>
  </Reliability>
  <ReplyHandling>
    <ReplyPattern>Response</ReplyPattern>    
    <ReceiptHandling>
      <UseNRRFormat>true</UseNRRFormat>
    </ReceiptHandling>
    <ErrorHandling>
      <UseSoapFault>false</UseSoapFault>
      <ResponseHttpCode>200</ResponseHttpCode>
    </ErrorHandling>
  </ReplyHandling>
  <ExceptionHandling>
    <NotifyMessageConsumer>true</NotifyMessageConsumer>
    <NotifyMethod>
      <Type>FILE</Type>
      <Parameters>
        <Parameter name="location" value=".\messages\exceptions\" />
      </Parameters>
    </NotifyMethod>
  </ExceptionHandling>
  <Security>
    <SigningVerification>
      <Signature>Ignored</Signature>
    </SigningVerification>
    <Decryption>
      <Encryption>Allowed</Encryption>
	    <CertificateFindCriteria>
        <CertificateFindType>FindBySubjectName</CertificateFindType>
        <CertificateFindValue>AccessPointA</CertificateFindValue>
      </CertificateFindCriteria>
    </Decryption>
  </Security>
  <MessagePackaging>
    <PartyInfo>
      <FromParty>
        <PartyIds>
          <PartyId>
            <Id>org:eu:europa:as4:example:accesspoint:B</Id>
          </PartyId>
        </PartyIds>
        <Role>Sender</Role>
      </FromParty>
      <ToParty>
        <PartyIds>
          <PartyId>
            <Id>org:eu:europa:as4:example:accesspoint:A</Id>
          </PartyId>
        </PartyIds>
        <Role>Receiver</Role>
      </ToParty>
    </PartyInfo>
    <CollaborationInfo>
      <AgreementReference>
        <Value>http://agreements.europa.org/agreement</Value>
      </AgreementReference>
      <Service>
        <Value>getting:started-staticsubmit</Value>
        <Type>org:europa:services</Type>
      </Service>
      <Action>eu:sample:04</Action>
    </CollaborationInfo>
  </MessagePackaging>
  <MessageHandling>
    <Forward>
	    <SendingPMode>forward-to-c1</SendingPMode>
    </Forward>
  </MessageHandling>
</PMode>