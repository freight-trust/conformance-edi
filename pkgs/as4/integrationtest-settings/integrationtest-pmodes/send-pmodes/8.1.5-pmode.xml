<?xml version="1.0" encoding="utf-8"?>
<PMode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
  xmlns="eu:edelivery:as4:pmode">
  <Id>8.1.5-pmode</Id>
  <AllowOverride>false</AllowOverride>
  <Mep>OneWay</Mep>
  <MepBinding>Push</MepBinding>
  <PushConfiguration>
    <Protocol>
      <Url>http://localhost:9090/msh</Url>
      <UseChunking>false</UseChunking>
      <UseHttpCompression>false</UseHttpCompression>
    </Protocol>
    <TlsConfiguration>
      <IsEnabled>false</IsEnabled>
      <TlsVersion>Tls12</TlsVersion>
      <ClientCertificateReference />
    </TlsConfiguration>
  </PushConfiguration>
  <Reliability>
    <ReceptionAwareness>
      <IsEnabled>false</IsEnabled>
      <RetryCount>5</RetryCount>
      <RetryInterval />
    </ReceptionAwareness>
  </Reliability>
  <ReceiptHandling>
    <NotifyMessageProducer>true</NotifyMessageProducer>
    <NotifyMethod>
      <Type>FILE</Type>
      <Parameters>
        <Parameter name="location" value=".\messages\receipts\" />
      </Parameters>
    </NotifyMethod>
  </ReceiptHandling>
  <ErrorHandling>
    <NotifyMessageProducer>false</NotifyMessageProducer>
    <NotifyMethod />
  </ErrorHandling>
  <ExceptionHandling>
    <NotifyMessageProducer>false</NotifyMessageProducer>
    <NotifyMethod />
  </ExceptionHandling>
  <Security>
    <Signing>
      <IsEnabled>false</IsEnabled>
      <CertificateFindCriteria>
        <CertificateFindType>FindBySubjectName</CertificateFindType>
        <CertificateFindValue>PartyA</CertificateFindValue>
      </CertificateFindCriteria>
      <KeyReferenceMethod>BSTReference</KeyReferenceMethod>
      <Algorithm>http://www.w3.org/2001/04/xmldsig-more#rsa-sha256</Algorithm>
      <HashFunction>http://www.w3.org/2001/04/xmlenc#sha256</HashFunction>
    </Signing>
    <Encryption>
      <IsEnabled>true</IsEnabled>
      <Algorithm>http://www.w3.org/2009/xmlenc11#aes128-gcm</Algorithm>
      <CertificateFindCriteria>
        <CertificateFindType>FindBySubjectName</CertificateFindType>
        <CertificateFindValue>AccessPointB</CertificateFindValue>
      </CertificateFindCriteria>
    </Encryption>
  </Security>
  <MessagePackaging>
    <UseAS4Compression>false</UseAS4Compression>
    <IsMultiHop>false</IsMultiHop>
    <IncludePModeId>true</IncludePModeId>
    <PartyInfo>
      <FromParty>
        <PartyIds>
          <PartyId>
            <Id>org:eu:europa:as4:example</Id>
          </PartyId>
        </PartyIds>
        <Role>Sender</Role>
      </FromParty>
      <ToParty>
        <PartyIds>
          <PartyId>
            <Id>org:holodeckb2b:example:company:B</Id>
          </PartyId>
        </PartyIds>
        <Role>Receiver</Role>
      </ToParty>
    </PartyInfo>
  </MessagePackaging>
</PMode>