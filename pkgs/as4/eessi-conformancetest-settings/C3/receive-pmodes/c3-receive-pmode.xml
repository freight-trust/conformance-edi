<?xml version="1.0"?>
<PMode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
  xmlns="eu:edelivery:as4:pmode">
  <Id>c3-receive-pmode</Id>
  <ReplyHandling>
    <ReplyPattern>Callback</ReplyPattern>
    <ReceiptHandling>
      <UseNRRFormat>true</UseNRRFormat>
    </ReceiptHandling>
    <ErrorHandling />
    <ResponseConfiguration>
      <Protocol>
        <Url>https://eessidev09.eessi.be/EESSI/BusinessMessaging/v0.0/Outbox/Service.svc</Url>
      </Protocol>
      <TlsConfiguration>
        <IsEnabled>true</IsEnabled>
        <TlsVersion>Tls12</TlsVersion>
        <ClientCertificateReference>
          <ClientCertificateFindType>FindBySerialNumber</ClientCertificateFindType>
          <ClientCertificateFindValue>1a00000173b2da572a2755f2e3000000000173</ClientCertificateFindValue>
        </ClientCertificateReference>
      </TlsConfiguration>
    </ResponseConfiguration>
    <ResponseSigning>
      <IsEnabled>true</IsEnabled>
      <CertificateFindCriteria>
        <CertificateFindType>FindBySerialNumber</CertificateFindType>
        <CertificateFindValue>1a00000174eb3567453664bc9f000000000174</CertificateFindValue>
      </CertificateFindCriteria>
      <KeyReferenceMethod>BSTReference</KeyReferenceMethod>
      <Algorithm>http://www.w3.org/2001/04/xmldsig-more#rsa-sha256</Algorithm>
      <HashFunction>http://www.w3.org/2001/04/xmlenc#sha256</HashFunction>
    </ResponseSigning>
  </ReplyHandling>
  <Security>
    <SigningVerification>
      <Signature>Allowed</Signature>
    </SigningVerification>
    <Decryption>
      <Encryption>Ignored</Encryption>
      <CertificateFindCriteria>
        <CertificateFindType>FindByThumbprint</CertificateFindType>
        <CertificateFindValue />
      </CertificateFindCriteria>
    </Decryption>
  </Security>
  <MessagePackaging>
    <PartyInfo>
      <FromParty>
        <PartyIds>
          <PartyId>
            <Id>UK:UK001</Id>
            <Type>urn:eu:europa:ec:dgempl:eessi:ir</Type>
          </PartyId>
        </PartyIds>
        <Role>urn:eu:europa:ec:dgempl:eessi:ir:institution</Role>
      </FromParty>
      <ToParty>
        <PartyIds>
          <PartyId>
            <Id>UK:UK010</Id>
            <Type>urn:eu:europa:ec:dgempl:eessi:ir</Type>
          </PartyId>
        </PartyIds>
        <Role>urn:eu:europa:ec:dgempl:eessi:ir:institution</Role>
      </ToParty>
    </PartyInfo>
    <CollaborationInfo>
      <Service>
        <Value>BusinessMessaging</Value>
        <Type>urn:eu:europa:ec:dgempl:eessi</Type>
      </Service>
      <Action>Send</Action>
    </CollaborationInfo>
  </MessagePackaging>
  <MessageHandling>
    <Deliver>
      <IsEnabled>true</IsEnabled>
      <PayloadReferenceMethod>
        <Type>FILE</Type>
        <Parameters>
          <Parameter name="location" value=".\messages\in\" />
        </Parameters>
      </PayloadReferenceMethod>
      <DeliverMethod>
        <Type>FILE</Type>
        <Parameters>
          <Parameter name="location" value=".\messages\in\" />
        </Parameters>
      </DeliverMethod>
    </Deliver>
  </MessageHandling>
</PMode>