<schema xmlns="http://xlate.io/EDISchema/v3">

  <transaction>
    <sequence>
      <segment type="S02" />

      <loop code="L0000" maxOccurs="5">
        <sequence>
          <segment type="S30" />
        </sequence>
      </loop>
    </sequence>
  </transaction>

  <implementation>
    <sequence>
      <segment type="S02" />

      <loop type="L0000" code="0000A" discriminator="2.2">
        <sequence>
          <segment type="S30">
            <sequence>
              <element position="1"></element>
              <composite position="2">
                <sequence>
                  <element position="1" />
                  <element position="2" /><!-- Missing enumeration -->
                </sequence>
              </composite>
            </sequence>
          </segment>
        </sequence>
      </loop>
    </sequence>
  </implementation>

  <elementType name="E999" base="string" />
  <elementType name="E888" base="string" />

  <elementType name="E777" base="identifier">
    <enumeration>
      <value>XX</value>
      <value>YY</value>
      <value>ZZ</value>
    </enumeration>
  </elementType>

  <compositeType name="C001">
    <sequence>
      <element type="E888" />
      <element type="E777" />
    </sequence>
  </compositeType>

  <segmentType name="S02">
    <sequence>
      <element type="E999" minOccurs="1" />
    </sequence>
  </segmentType>

  <segmentType name="S30">
    <sequence>
      <element type="E999" />
      <composite type="C001" minOccurs="1" />
    </sequence>
  </segmentType>
</schema>
